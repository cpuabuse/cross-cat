{
	"compilerOptions": {
		"noImplicitAny": true,
		"target": "es2018",
		"module": "commonjs",
		"lib": ["es2020"],
		"strict": true,
		"moduleResolution": "node",
		"declaration": true,
		"sourceMap": true,
		"esModuleInterop": true
	},
	"typedocOptions": {
		"mode": "modules",
		"out": "docs"
	},
	"include": ["**/*"],
	"exclude": []
}
